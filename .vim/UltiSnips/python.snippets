snippet imp "import" i
import ${1:module}
endsnippet

snippet fimp "from $1 import $2" i
from ${1:name} import ${2:module}
endsnippet

snippet w "while" i
while ${1:condition}:
    ${2:expressions}
endsnippet

snippet f "for" i
for ${1:condition}:
    ${2:expressions}
endsnippet

snippet i "if" i
if ${1:condition}:
    ${2:expressions}
endsnippet


snippet ie "if else" i
if ${1:condition}:
    ${2:expressions}
${3:condition}
endsnippet

snippet p "print" i
print(${1:message})
endsnippet
